{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@uiw/react-codemirror": "^2.3.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.3",
    "chakra-ui": "^0.3.9",
    "codemirror": "^5.56.0",
    "codemirror-spell-checker": "*",
    "cytoscape": "^3.15.0",
    "emotion": "^10.0.27",
    "jwt-decode": "^2.2.0",
    "lint-staged": "^10.0.7",
    "markdown-it": "^11.0.0",
    "marked": "^1.1.1",
    "monaco-editor": "^0.20.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.1",
    "simplemde": "^1.11.2",
    "spark-md5": "^3.0.1",
    "umi": "^3.2.3",
    "vis-network-react": "^1.3.3",
    "yorkie": "^2.0.0"
  }
}
